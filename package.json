{
  "name": "@lautag/friday",
  "version": "0.0.1",
  "description": "Javascript implementation of error reporting for laut.ag",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "browser": "browser/browser-index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "test": "mocha",
    "make:docs": "typedoc",
    "make:types": "rm -rf types/ && tsc -t es6 -d --emitDeclarationOnly --allowJs --outdir ./types --allowSyntheticDefaultImports --moduleResolution Node ./src/**/*",
    "build:es": "./scripts/compile.sh module",
    "build:cjs": "./scripts/compile.sh cjs",
    "build:browser": "./scripts/compile.sh browser",
    "build": "npm run build:es && npm run build:cjs && npm run build:browser",
    "release": "./scripts/publish.sh"
  },
  "directories": {
    "test": "tests",
    "example": "examples"
  },
  "author": "laut.ag",
  "contributors": [
    "Vern Hockney"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@sentry/browser": "^5.15.5",
    "@sentry/integrations": "^5.15.5",
    "@sentry/node": "^5.15.5",
    "@sentry/types": "^5.15.5",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.2",
    "@types/sinon": "^9.0.0",
    "chai": "^4.2.0",
    "mocha": "^7.1.2",
    "sentry-testkit": "^3.2.1",
    "sinon": "^9.0.2",
    "typedoc": "^0.17.6",
    "typescript": "^3.8.3",
    "wait-for-expect": "^3.0.2"
  },
  "dependencies": {}
}
