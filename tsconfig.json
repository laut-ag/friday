{
  "compilerOptions": {
    "module": "commonjs",
    "target": "esnext",
    "moduleResolution": "node",
    "isolatedModules": true,
    "allowJs": true,
    "noEmit": false,
    "strict": true,
    "outDir": "types",
    "esModuleInterop": true,
    "sourceMap": true
  },
  "lib": [  "es2015.collection" ],
  "exclude": [
    "node_modules",
    "tests/babel-register.js"
  ],
  "include": [
    "src"
  ],
  "typedocOptions": {
    "mode": "file",
    "out": "docs",
    "includeVersion": true,
    "readme": "README.md"
  }
}
